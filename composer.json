{
    "name": "snoke/doctrine-softdelete",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": { "Snoke\\SoftDelete\\": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "5.2.*"
        }
    },
    "require": {
        "php": ">=8.2",
        "doctrine/doctrine-bundle": ">=2.12",
        "doctrine/orm": "3.*",
        "symfony/framework-bundle": "7.1.*"
    }
}
